    "babel-loader": "^7.1.2",
    "babel-register": "^6.8.0",
    "colors": "^1.1.2",
    "css-loader": "^0.28.7",
    "eslint": "^4.8.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-watch": "^3.1.2",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "open": "^0.0.5",
